   "version": "0.2.0",
   "configurations": [
      {
         "type": "node",
         "request": "launch",
         "name": "Launch Program",
         "program": "${workspaceFolder}/server/dist/main/index.js"
      },
      {
         "type": "node",
         "request": "launch",
         "name": "Gulp test",
         "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
         "args": [
            "test-only"
         ],
         "cwd": "${workspaceFolder}",
         "outFiles": [
            "${workspaceFolder}/server/dist/**/*.js"
         ],
         "sourceMaps": true
      },
      {
         "type": "node",
         "request": "launch",
         "name": "Gulp build",
         "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
         "args": [
            "build"
         ],
         "cwd": "${workspaceFolder}"
      }
   ]
}
